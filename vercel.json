{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "resume.html",
      "use": "@vercel/static"
    },
    {
      "src": "resume.css",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    }

  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/resume.html",
      "dest": "/resume.html"
    },
    {
      "src": "/resume.css",
      "dest": "/resume.css"
    },
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
